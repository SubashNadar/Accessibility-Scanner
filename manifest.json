{
  "manifest_version": 3,
  "name": "Cody Accessibility Checker",
  "version": "0.1.0",
  "description": "Analyzes webpage accessibility using Cody AI.",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://laxtst-insg-001.office.cyberu.com/"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
